{
  "name": "preact-easy-title",
  "version": "0.0.1",
  "main": "index.js",
  "description": "Easy title for Preact",
  "keywords": [
    "preact-easy-title",
    "preact",
    "title"
  ],
  "author": "ryuapp",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --jsx preserve -t es2020 --outDir dist --noEmit false&npm run copy",
