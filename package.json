    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "preact": "^10.11.3"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.4.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "license": "MIT"
}
